# FocusTube Server Environment Variables
# Copy this file to .env and fill in your actual values

# OpenAI API Key (required for AI classification)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Supabase Configuration (required for user data and plan management)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Stripe Configuration (required for payments)
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key-here
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret-here

# Stripe Price IDs (create products in Stripe dashboard, then add price IDs here)
STRIPE_PRICE_MONTHLY=price_your-monthly-price-id
STRIPE_PRICE_ANNUAL=price_your-annual-price-id
STRIPE_PRICE_LIFETIME=price_your-lifetime-price-id

# Server Configuration (optional)
PORT=3000
NODE_ENV=development
CORS_ORIGIN=http://localhost:3000
